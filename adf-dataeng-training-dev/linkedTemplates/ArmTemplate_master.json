{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dataeng-training-dev"
		},
		"ls_Adf_to_ADLS_Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_Adf_to_ADLS_Gen2'"
		},
		"ls_onprem_filesystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onprem_filesystem'"
		},
		"ls_Adf_to_ADLS_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stadlsgen2dataenggdev2.dfs.core.windows.net/"
		},
		"ls_DataEngg_keyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvdataenggsecrets.vault.azure.net/"
		},
		"ls_onprem_filesystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\projects\\files"
		},
		"ls_onprem_filesystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "ynhemanth@hotmail.com"
		},
		"ls_onprem_sqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "HEMANTHPC\\SQLEXPRESS"
		},
		"ls_onprem_sqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mbdbx101"
		},
		"ls_onprem_sqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ynhemanth"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dataeng-training-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Adf_to_ADLS_Gen2_accountKey": {
						"value": "[parameters('ls_Adf_to_ADLS_Gen2_accountKey')]"
					},
					"ls_onprem_filesystem_password": {
						"value": "[parameters('ls_onprem_filesystem_password')]"
					},
					"ls_Adf_to_ADLS_Gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_Adf_to_ADLS_Gen2_properties_typeProperties_url')]"
					},
					"ls_DataEngg_keyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_DataEngg_keyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_onprem_filesystem_properties_typeProperties_host": {
						"value": "[parameters('ls_onprem_filesystem_properties_typeProperties_host')]"
					},
					"ls_onprem_filesystem_properties_typeProperties_userId": {
						"value": "[parameters('ls_onprem_filesystem_properties_typeProperties_userId')]"
					},
					"ls_onprem_sqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_onprem_sqlserver_properties_typeProperties_server')]"
					},
					"ls_onprem_sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_onprem_sqlserver_properties_typeProperties_database')]"
					},
					"ls_onprem_sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_onprem_sqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}